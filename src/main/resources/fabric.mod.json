{
  "schemaVersion": 1,
  "id": "lynx",
  "version": "${version}",

  "name": "LYNX",
  "description": "LYNX is a free and powerful path-finding bot that lets you navigate the vast landscapes of Minecraft without any user intervention.",
  "authors": [
    "StylexTV"
  ],
  "contact": {
    "homepage": "https://playlynx.github.io/",
    "sources": "https://github.com/StylexTV/LYNX",
    "issues": "https://github.com/StylexTV/LYNX/issues"
  },

  "license": "GPL-3.0",
  "icon": "assets/lynx/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "de.stylextv.lynx.Lynx"
    ]
  },
  "mixins": [
    "lynx.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=16"
  }
}
