  "authors": [
    "StylexTV"
  ],
  "contact": {
    "homepage": "https://maplemc.github.io/",
    "sources": "https://github.com/StylexTV/Maple",
    "issues": "https://github.com/StylexTV/Maple/issues"
  },

  "license": "GPL-3.0",
  "icon": "assets/maple/icon.png",

  "environment": "client",
  "entrypoints": {
